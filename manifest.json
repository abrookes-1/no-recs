{
    "name": "No Recs",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["*://www.youtube.com/*"],
            "js": ["options.js"],
            "run_at": "document_start"
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "No Recs"
    },
    "permissions": [
        "storage"
    ]
}